{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "exampleGlobalVariable": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "eqeqeq": 0,
    "no-console": 0,
    "semi": 1
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "plugins": [
    "node"
  ]
}
